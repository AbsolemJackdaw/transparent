{
  "schema_version": 1,
  "mixin": [
    "transparent.mixins.json",
    "transparent-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "transparent",
    "version": "${version}",
    "metadata": {
      "name": "Transparent",
      "description": "Allows resource packs to make entities support transparency.",
      "contributors": {
        "Trikzon": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/transparent",
        "issues": "https://github.com/trikzon/transparent/issues",
        "sources": "https://github.com/trikzon/transparent"
      },
      "license": "MIT",
      "icon": "assets/transparent/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "client_init": [
        "com.trikzon.transparent.client.quilt.TransparentClientQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      }
    ]
  }
}
